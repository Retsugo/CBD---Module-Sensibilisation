rCertificatBigData <- function(n=1000,var_mode="Y",law="U"){
if (var_mode=="Y"){
if (law=="U"){
X1 <- runif(n)
X2 <- runif(n)
Samples <- X1+X2}
else{
X1 <- rnorm(n)
X2 <- rnorm(n)
Samples <- X1+X2}
}
if (var_mode=="Z1"){
if (law=="U"){
X1 <- runif(n)
X2 <- runif(n)
Samples <- X1*X2}
else{
X1 <- rnorm(n)
X2 <- rnorm(n)
Samples <- X1*X2}
}
if (var_mode=="Z2"){
if (law=="U"){
X1 <- runif(n)
X2 <- runif(n)
Samples <- X1/X2}
else{
X1 <- rnorm(n)
X2 <- rnorm(n)
Samples <- X1/X2}
}
return(Samples)
}
rCertificatBigData(1000)
rCertificatBigData(1000,var_mode="Y")
rCertificatBigData(1000,var_mode="Z1")
